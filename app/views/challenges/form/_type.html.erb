<div class="row">
  <%= form_for [@organizer,@challenge],
        validate: true,
        html: { class: "dbl-col" } do |f| %>
        <fieldset class="checkbox">
          <%= f.check_box :private_challenge, {}, "true", "false" %>
          <label for="">Private challenge</label>
        </fieldset>
      <div class="twin-container">
        <div class="twin-field">

        </div>
        <div class="twin-field">
        </div>
      </div>
    <%= render partial: 'challenges/form/submit_buttons', locals: { f: f } %>
  <% end %>
</div>
