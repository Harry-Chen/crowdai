<div class="section-container nested-fields">
  <div class="twin-container">
    <div class="twin-field">
      <label>Image</label>
      <div class="upload">
        <div class='markdown-wrap'>
          <%= concept(Image::Cell,f.object,klass: 'img-logo') %>
        </div>
        <%= f.file_field :image_file, class: 'file-upload' %>
        <% if f.object.image_file.present? %>
          <%#= link_to 'Delete', challenge_remove_image_path(@challenge) %>
        <% end %>
        <span>Your image will be cropped if necessary. 5MB limit</span>
      </div>
    </div>
    <div class="twin-field">
      <label>Partner Url</label>
      <%= f.text_field :partner_url %>
    </div>
  </div>
  <%= link_to_remove_association "remove", f, class: 'pull-right' %>
</div>
